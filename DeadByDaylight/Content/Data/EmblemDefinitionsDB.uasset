        UI/Icons/Emblems/emblemIcon_gatekeeper_gold.png
        , 
        UI/Icons/Emblems/emblemIcon_gatekeeper_platinum.png
    ]
    EmblemEvaluation: EEmblemEvaluation::Local
    Enabled: True
    EmblemProgressionData: [
        {
            EmblemProgressionType: EEmblemProgressionType::KillerGatekeeperStartingValue
            DefaultDescription: Gatekeeper Starting Value
            DescriptionsByQuality: [ ]
        }
        , 
        {
            EmblemProgressionType: EEmblemProgressionType::KillerGatekeeperGeneratorDefense
            DefaultDescription: Generator Defense
            DescriptionsByQuality: [ ]
        }
        , 
        {
            EmblemProgressionType: EEmblemProgressionType::KillerGatekeeperGatesClosed
            DefaultDescription: Gates Closed
            DescriptionsByQuality: [ ]
        }
    ]
